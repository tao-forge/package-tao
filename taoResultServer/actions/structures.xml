<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE extension SYSTEM "../../tao/doc/structures.dtd">
<structures>
    <structure id="resultserver" name="Result Server" visible="true">
		<description>Manage Result Servers</description>
    <sections>
    <section id="manage_resultservers" name="Manage result servers" url="/taoResultServer/ResultServerMgt/index">
				<trees>
					<tree name="Result Servers library"
						className="Result Server"
						dataUrl="/taoResultServer/ResultServerMgt/getResultServers"
						editClassUrl="/taoResultServer/ResultServerMgt/editResultServerClass"
						editInstanceUrl="/taoResultServer/ResultServerMgt/editResultServer"
						addInstanceUrl="/taoResultServer/ResultServerMgt/addResultServer"
						addSubClassUrl="/taoResultServer/ResultServerMgt/addResultServerClass"
						deleteUrl="/taoResultServer/ResultServerMgt/delete"
						duplicateUrl="/taoResultServer/ResultServerMgt/cloneInstance"
						moveInstanceUrl="/taoResultServer/ResultServerMgt/moveInstance"
					/>
				</trees>
				<actions>
					<action name="search" url="/taoResultServer/ResultServerMgt/search" context="*" />
					<action name="new class" js="generisActions.subClass" url="/taoResultServer/ResultServerMgt/addResultServerClass" context="class" />
					<action name="new result server" js="generisActions.instanciate" url="/taoResultServer/ResultServerMgt/addResultServer" context="class" />
					<action name="delete" js="generisActions.removeNode" url="/taoResultServer/ResultServerMgt/delete" context="resource" />
					<action name="duplicate" js="generisActions.duplicateNode" url="/taoResultServer/ResultServerMgt/cloneInstance" context="instance" />
					<action name="move" js="generisActions.moveNode" url="" context="instance" />
					<action name="export" url="/tao/Export/index"  context="*" />
					<action name="import" url="/tao/Import/index"  context="class" />
				</actions>
			</section>
     </sections>
    </structure>
</structures>