				<baseValue baseType="identifier">completed</baseValue>
			</match>
			<setOutcomeValue identifier="step">
				<baseValue baseType="identifier">step8</baseValue>
			</setOutcomeValue>
		</outcomeIf>
	</outcomeCondition>
	<outcomeCondition>
		<outcomeIf>
			<match>
				<variable identifier="Q09.completionStatus"/>
				<baseValue baseType="identifier">completed</baseValue>
			</match>
			<setOutcomeValue identifier="step">
				<baseValue baseType="identifier">step9</baseValue>
			</setOutcomeValue>
		</outcomeIf>
	</outcomeCondition>
