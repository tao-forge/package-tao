				<baseValue baseType="identifier">completed</baseValue>
			</match>
			<setOutcomeValue identifier="step">
				<baseValue baseType="identifier">step4</baseValue>
			</setOutcomeValue>
		</outcomeIf>
	</outcomeCondition>
