		</assessmentItemRef>
		
		<assessmentItemRef identifier="Q09" href="./Q09/qti.xml">
			<branchRule target="TP02">
				<equalRounded roundingMode="decimalPlaces" figures="1">
					<variable identifier="Q09.SCORE"/>
					<baseValue baseType="float">0.0</baseValue>
				</equalRounded>
			</branchRule>
			<branchRule target="TP03">
				<equalRounded roundingMode="decimalPlaces" figures="1">
					<variable identifier="Q09.SCORE"/>
					<baseValue baseType="float">1.0</baseValue>
				</equalRounded>	
			</branchRule>
		</assessmentItemRef>
        </assessmentSection>
    </testPart>
    <testPart navigationMode="linear" submissionMode="individual" identifier="TP02">
    	<assessmentSection identifier="S02" title="You Failed!" visible="true">
		<rubricBlock id="testFailure" view="candidate">
			<h1>You Failed!</h1>
			<p>I am sorry, candidate. You failed!</p>
			<div>
				<feedbackBlock outcomeIdentifier="step" showHide="show" identifier="step1">
					<img src="img/failure1.png" alt="Test Map - Q01 Failure"/>
				</feedbackBlock>
	
				<feedbackBlock outcomeIdentifier="step" showHide="show" identifier="step2">
					<img src="img/failure2.png" alt="Test Map - Q02 Failure"/>
				</feedbackBlock>
	
				<feedbackBlock outcomeIdentifier="step" showHide="show" identifier="step3">
					<img src="img/failure3.png" alt="Test Map - Q03 Failure"/>
				</feedbackBlock>
	
				<feedbackBlock outcomeIdentifier="step" showHide="show" identifier="step4">
					<img src="img/failure4.png" alt="Test Map - Q04 Failure"/>
				</feedbackBlock>
	
				<feedbackBlock outcomeIdentifier="step" showHide="show" identifier="step5">
					<img src="img/failure5.png" alt="Test Map - Q05 Failure"/>
				</feedbackBlock>
	
				<feedbackBlock outcomeIdentifier="step" showHide="show" identifier="step6">
					<img src="img/failure6.png" alt="Test Map - Q06 Failure"/>
				</feedbackBlock>
	
				<feedbackBlock outcomeIdentifier="step" showHide="show" identifier="step7">
