					<variable identifier="Q06.SCORE"/>
					<baseValue baseType="float">0.0</baseValue>
				</equalRounded>
			</branchRule>
		</assessmentItemRef>

		<assessmentItemRef identifier="Q07" href="./Q07/qti.xml">
			<branchRule target="TP02">
				<equalRounded roundingMode="decimalPlaces" figures="1">
					<variable identifier="Q07.SCORE"/>
					<baseValue baseType="float">0.0</baseValue>
				</equalRounded>
			</branchRule>
		</assessmentItemRef>

		<assessmentItemRef identifier="Q08" href="./Q08/qti.xml">
			<branchRule target="TP02">
				<equalRounded roundingMode="decimalPlaces" figures="1">
					<variable identifier="Q08.SCORE"/>
