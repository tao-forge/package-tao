<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE extension SYSTEM "../../tao/doc/structures.dtd">
<structures>
	<structure id="results" name="Results" level="5" visible="true">
		<description>View and format the collected results.</description>
	<sections>
	    <section id="manage_results" name="Manage Results" url="/taoOutcomeUi/Results/index">
		<trees>
		    <tree name="Results library"
			className="Result"
			dataUrl="/taoOutcomeUi/Results/getOntologyData"
			addSubClassUrl="/taoOutcomeUi/Results/addSubClass"
			editClassUrl="/taoOutcomeUi/Results/index"
			editInstanceUrl="/taoOutcomeUi/Results/viewResult"
			moveInstanceUrl="/taoOutcomeUi/Results/moveInstance"
		    />
		</trees>
		<actions>
		<action name="new class" js="subClass" url="/taoOutcomeUi/Results/addSubClass" context="class" />
		<action name="move" js="moveNode" url="" context="instance" />
		<action name="delete" js="removeNode" url="/taoOutcomeUi/Results/delete" context="resource" />
		<action name="summary report" url="/taoOutcomeUi/SimpleReport/build" context="class" />
		</actions>
	    </section>
	    <section id="inspect_results" name="Inspect Results" url="/taoOutcomeUi/InspectResults/index" />
	  </sections>
	</structure>
</structures>
