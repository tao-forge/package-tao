<?xml version="1.0" encoding="utf-8"?>
<extension name="Results" url="/taoResults/Results/index" level="5">
	<sections>
		<section name="Manage results" url="/taoResults/Results/index">
			<trees>
				<tree name="Results library" 
					dataUrl="/taoResults/Results/getResults" 
					editClassUrl="/taoResults/Results/editResultClass"
					editInstanceUrl="/taoResults/Results/editResult"
					addInstanceUrl="/taoResults/Results/addResult"
					addSubClassUrl="/taoResults/Results/addResultClass"
					deleteUrl="/taoResults/Results/delete"
					duplicateUrl="/taoResults/Results/cloneResult"
				/>
			</trees>
			<actions>
				<action name="new class" js="subClass" url="/taoResults/Results/addResultClass" context="class" />
				<action name="new result" js="instanciate" url="/taoResults/Results/addResult" context="class" />
				<action name="delete" js="removeNode" url="/taoResults/Results/delete" context="resource" />
				<action name="duplicate" js="cloneNode" url="/taoResults/Results/cloneResult" context="instance" />
				<action name="edit result" url="/taoResults/Results/editResult" context="instance" />
				<action name="new property" js="addProperty" url="/taoResults/Results/addClassProperty" context="class" />
				<action name="create table" url="/taoResults/Results/createTable"  context="*" />
				<action name="import" url="/taoResults/Results/import"  context="class" />
				<action name="export" url="/taoResults/Results/export"  context="class" reload="true" />
			</actions>
		</section>
	</sections>
</extension>