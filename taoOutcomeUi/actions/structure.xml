<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE extension SYSTEM "../../tao/doc/structure.dtd">
<extension name="Results" url="/taoResults/Results/index" level="5">
	<description>View and format the collected results.</description>
    <sections>
        <section id="manage_results" name="Manage results" url="/taoResults/Results/index">
            <trees>
                <tree name="Results library"
					dataUrl="/taoResults/Results/getOntologyData" 
					editClassUrl="/taoResults/Results/editResultClass"
					editInstanceUrl="/taoResults/Results/editResult"
					addInstanceUrl="/taoResults/Results/addInstance"
					addSubClassUrl="/taoResults/Results/addResultClass"
					deleteUrl="/taoResults/Results/delete"
					duplicateUrl="/taoResults/Results/cloneInstance"
					moveInstanceUrl="/taoResults/Results/moveInstance"
				/>
            </trees>
            <actions>
                
                <action name="create direct table" url="/taoResults/Results/createDirectTable"  context="class" />
                <action name="create score table" url="/taoResults/Results/createScoreTable"  context="class" />
                <action name="create table" url="/taoResults/Results/createTable"  context="class" />
            </actions>
        </section>


      </sections>
</extension>