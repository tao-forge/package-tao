<?xml version="1.0" encoding="utf-8"?>
<extension name="Results" url="/taoResults/Results/index" level="5">
	<sections>
		<section name="Manage results" url="/taoResults/Results/index">
			<trees>
				<tree name="Results library" 
					dataUrl="/taoResults/Results/getOntologyData" 
					editClassUrl="/taoResults/Results/editResultClass"
					editInstanceUrl="/taoResults/Results/editResult"
					addInstanceUrl="/taoResults/Results/addInstance"
					addSubClassUrl="/taoResults/Results/addResultClass"
					deleteUrl="/taoResults/Results/delete"
					duplicateUrl="/taoResults/Results/cloneInstance"
				/>
			</trees>
			<actions>
				<action name="search" url="/taoResults/Results/search" context="*" />
				<action name="new class" js="GenerisAction.subClass" url="/taoResults/Results/addResultClass" context="class" />
				<action name="new result" js="GenerisAction.instanciate" url="/taoResults/Results/addInstance" context="class" />
				<action name="delete" js="GenerisAction.removeNode" url="/taoResults/Results/delete" context="resource" />
				<action name="duplicate" js="GenerisAction.duplicateNode" url="/taoResults/Results/cloneInstance" context="instance" />
				<action name="edit result" url="/taoResults/Results/editResult" context="instance" />
				<action name="new property" js="GenerisAction.addProperty" url="/taoResults/Results/addClassProperty" context="class" />
				<action name="create table" js="GenerisAction.resultTable" url="/taoResults/Results/createTable"  context="class" />
				<action name="import" url="/taoResults/Results/import"  context="class" />
				<action name="export" url="/taoResults/Results/export"  context="class" reload="true" />
			</actions>
		</section>
	</sections>
</extension>