<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE services SYSTEM "../../tao/doc/sas.dtd">
<services>
	<description>Results services</description>
	<service>
		<name>Results searching</name>
		<description>Search results instance form</description>
		<location url="/taoResults/SaSResults/search" />
		<return>
			<param key="resultUris" />
		</return>
	</service>	
	<service>
		<name>Result Table</name>
		<description>Open the UTR table on the results</description>
		<location url="/taoResults/SaSResults/createTable">
			<param key="classUri" value="^resultClassUri" />
		</location>
	</service>	
</services>