<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE services SYSTEM "../../tao/doc/sas.dtd">
<services>
	<description>Other Delivery services</description>
	
	<!-- generic web service -->
	<service>
		<name>Web Service</name>
		<description>Embed a generic web service accessible via url in an iframe</description>
		<location url="/taoDelivery/WebService/index">
			<param key="url" value="^webServiceUrl" />
			<param key="height" value="98%" />
			<param key="width" value="98%" />
		</location>
	</service>
	
	<!-- item container service -->
	<service>
		<name>Item Runner</name>
		<description>Embed a run an item into a Delivery. This service initialize the TAO Apis and contexts</description>
		<location url="/taoDelivery/ItemDeliveryService/index">
			<param key="itemUri" value="^itemUri" />
			<param key="testUri" value="^testUri" />
			<param key="deliveryUri" value="^deliveryUri" />
		</location>
	</service>
	
</services>