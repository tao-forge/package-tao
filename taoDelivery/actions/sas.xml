<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE services SYSTEM "../../tao/doc/sas.dtd">
<services>
	<service>
		<name>Search Delivery</name>
		<description>	 - Search instance form 	</description>
		<location url="/taoDelivery/SaSDelivery/search" />
		<return>
			<param key="deliveryUris" />
		</return>
	</service>	
	<service>
		<name>Add a delivery</name>
		<description>	 - add a new delivery  - redirected to the edit form	</description>
		<location url="/taoDelivery/SaSDelivery/sasAddInstance">
			<param key="classUri" value="^deliveryClassUri" />
		</location>
		<return>
			<param key="deliveryUri" />
		</return>
	</service>
	<service>
		<name>Edit a delivery</name>
		<description>	 - delivery instance form	</description>
		<location url="/taoDelivery/SaSDelivery/sasEditInstance">
			<param key="uri" value="^deliveryUri" />
			<param key="classUri" value="^deliveryClassUri" />
		</location>
	</service>
	<service>
		<name>Delivery full edit</name>
		<description>	 - delivery instance form  - exclude testees  - select campaign	</description>
		<location url="/taoDelivery/SaSDelivery/editDelivery">
			<param key="uri" value="^deliveryUri" />
			<param key="classUri" value="^deliveryClassUri" />
		</location>
	</service>
	<service>
		<name>Edit devlivery classes</name>
		<description>	 - Class editor	</description>
		<location url="/taoDelivery/SaSDelivery/editDeliveryClass">
			<param key="classUri" value="^deliveryClassUri" />
		</location>
	</service>
	<service>
		<name>Exclude subject from delivery</name>
		<description>	 - tree form to select subject to exclude	</description>
		<location url="/taoDelivery/SaSDelivery/excludeSubjects">
			<param key="uri" value="^deliveryUri" />
			<param key="classUri" value="^deliveryClassUri" />
		</location>
		<return>
			<param key="subjectUris" />
		</return>
	</service>
	<service>
		<name>Select related campaign</name>
		<description>	 - tree form to select delivery campaigns	</description>
		<location url="/taoDelivery/SaSDelivery/selectCampaign">
			<param key="uri" value="^deliveryUri" />
			<param key="classUri" value="^deliveryClassUri" />
		</location>
		<return>
			<param key="campaignUris" />
		</return>
	</service>
	<service>
		<name>Delete a delivery</name>
		<description>	 - Delete an instance	</description>
		<location url="/taoDelivery/SaSDelivery/sasDeleteInstance">
			<param key="uri" value="^deliveryUri" />
			<param key="classUri" value="^deliveryClassUri" />
		</location>
	</service>
			
	<service>
		<name>Search Campaign</name>
		<description>	 - Search instance form 	</description>
		<location url="/taoDelivery/SaSCampaign/search" />
		<return>
			<param key="campaignUris" />
		</return>
	</service>
	<service>
		<name>Add a campaign</name>
		<description>	 - add a new campaign  - redirected to the edit form	</description>
		<location url="/taoDelivery/SaSCampaign/sasAddInstance">
			<param key="classUri" value="^campaignClassUri" />
		</location>
		<return>
			<param key="campaignUri" />
		</return>
	</service>
	<service>
		<name>Edit a campaign</name>
		<description>	 - campaign instance form	</description>
		<location url="/taoDelivery/SaSCampaign/sasEditInstance">
			<param key="uri" value="^campaignUri" />
			<param key="classUri" value="^campaignClassUri" />
		</location>
	</service>
	<service>
		<name>Campaign full edit</name>
		<description>	 - campaign instance form  - select deliveries	</description>
		<location url="/taoDelivery/SaSCampaign/editCampaign">
			<param key="uri" value="^campaignUri" />
			<param key="classUri" value="^campaignClassUri" />
		</location>
	</service>
	<service>
		<name>Edit campaign classes</name>
		<description>	 - Class editor	</description>
		<location url="/taoDelivery/SaSCampaign/editCampaignClass">
			<param key="classUri" value="^campaignClassUri" />
		</location>
	</service>
	<service>
		<name>Select Campaign related delivery</name>
		<description>	 - tree form to select  campaign deliveries	</description>
		<location url="/taoDelivery/SaSCampaign/selectDeliveries">
			<param key="uri" value="^campaignUri" />
			<param key="classUri" value="^campaignClassUri" />
		</location>
	</service>
	<service>
		<name>Delete a Campaign</name>
		<description>	 - Delete an instance	</description>
		<location url="/taoDelivery/SaSCampaign/sasDeleteInstance">
			<param key="uri"  value="^campaignUri" />
			<param key="classUri" value="^campaignClassUri" />
		</location>
	</service>
			
	<service>
		<name>Search ResultServer</name>
		<description>	 - Search instance form 	</description>
		<location url="/taoDelivery/SaSResultServer/search" />
		<return>
			<param key="resultServerUris" />
		</return>
	</service>
	<service>
		<name>Add a ResultServer</name>
		<description>	 - add a new ResultServer  - redirected to the edit form	</description>
		<location url="/taoDelivery/SaSResultServer/sasAddInstance">
			<param key="classUri" value="^resultServerClassUri" />
		</location>
		<return>
			<param key="resultServerUri" />
		</return>
	</service>
	<service>
		<name>Edit a ResultServer</name>
		<description>	 - resultServer instance form	</description>
		<location url="/taoDelivery/SaSResultServer/sasEditInstance">
			<param key="uri" value="^resultServerUri" />
			<param key="classUri" value="^resultServerClassUri" />
		</location>
	</service>
	<service>
		<name>ResultServer full edit</name>
		<description>	 - resultserver instance form  - select deliveries	</description>
		<location url="/taoDelivery/SaSResultServer/editResultServer">
			<param key="uri" value="^resultServerUri" />
			<param key="classUri" value="^resultServerClassUri" />
		</location>
	</service>
	<service>
		<name>Edit ResultServer classes</name>
		<description>	 - Class editor	</description>
		<location url="/taoDelivery/SaSResultServer/editResultServerClass">
			<param key="classUri" value="^resultServerClassUri" />
		</location>
	</service>
	<service>
		<name>Select ResultServer related delivery</name>
		<description>	 - tree form to select  result server deliveries	</description>
		<location url="/taoDelivery/SaSResultServer/selectDeliveries">
			<param key="uri" value="^resultServerUri" />
			<param key="classUri" value="^resultServerClassUri" />
		</location>
		<return>
			<param key="deliveryUris" />
		</return>
	</service>
	<service>
		<name>Delete a ResultServer</name>
		<description>	 - Delete an instance	</description>
		<location url="/taoDelievry/SaSResultServer/sasDeleteInstance">
			<param key="uri" value="^resultServerUri" />
			<param key="classUri" value="^resultServerClassUri" />
		</location>
	</service>
</services>