<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE services SYSTEM "../../tao/doc/sas.dtd">
<services>
	<service>
		<name>Deliveries searching</name>
		<description>Search deliveries instances</description>
		<location url="/taoDelivery/SaSDelivery/search" />
		<return>
			<param key="deliveryUris" />
		</return>
	</service>	
	<service>
		<name>Delivery add</name>
		<description>Add a new delivery and redirect to the edit form</description>
		<location url="/taoDelivery/SaSDelivery/sasAddInstance">
			<param key="classUri" value="^deliveryClassUri" />
		</location>
		<return>
			<param key="deliveryUri" />
		</return>
	</service>
	<service>
		<name>Delivery editor</name>
		<description>Edit delivery properties</description>
		<location url="/taoDelivery/SaSDelivery/sasEditInstance">
			<param key="uri" value="^deliveryUri" />
			<param key="classUri" value="^deliveryClassUri" />
		</location>
	</service>
	<service>
		<name>Delivery full editor</name>
		<description>Full edit form: edit delivery properties, exclude testees and select campaigns</description>
		<location url="/taoDelivery/SaSDelivery/editDelivery">
			<param key="uri" value="^deliveryUri" />
			<param key="classUri" value="^deliveryClassUri" />
		</location>
	</service>
	<service>
		<name>Devlivery class editor</name>
		<description>Edit a Delivery (sub)class: add/edit/delete class properties</description>
		<location url="/taoDelivery/SaSDelivery/editDeliveryClass">
			<param key="classUri" value="^deliveryClassUri" />
		</location>
	</service>
	<service>
		<name>Delivery exclude subject</name>
		<description>Select subject to exclude of the delivery</description>
		<location url="/taoDelivery/SaSDelivery/excludeSubjects">
			<param key="uri" value="^deliveryUri" />
			<param key="classUri" value="^deliveryClassUri" />
		</location>
		<return>
			<param key="subjectUris" />
		</return>
	</service>
	<service>
		<name>Delivery related campaigns</name>
		<description>Select delivery campaigns</description>
		<location url="/taoDelivery/SaSDelivery/selectCampaign">
			<param key="uri" value="^deliveryUri" />
			<param key="classUri" value="^deliveryClassUri" />
		</location>
		<return>
			<param key="campaignUris" />
		</return>
	</service>
	<service>
		<name>Delivery removing</name>
		<description>Delete a delivery instance</description>
		<location url="/taoDelivery/SaSDelivery/sasDeleteInstance">
			<param key="uri" value="^deliveryUri" />
			<param key="classUri" value="^deliveryClassUri" />
		</location>
	</service>
	<service>
		<name>Delivery compilation</name>
		<description>Compile all the tests composing the delivery</description>
		<location url="/taoDelivery/SaSDelivery/compileView">
			<param key="uri" value="^deliveryUri" />
			<param key="classUri" value="^deliveryClassUri" />
		</location>
	</service>
			
	<service>
		<name>Campaigns searching</name>
		<description>Search campaigns instances</description>
		<location url="/taoDelivery/SaSCampaign/search" />
		<return>
			<param key="campaignUris" />
		</return>
	</service>
	<service>
		<name>Campaign add</name>
		<description>Add a new campaign and redirect to the edit form</description>
		<location url="/taoDelivery/SaSCampaign/sasAddInstance">
			<param key="classUri" value="^campaignClassUri" />
		</location>
		<return>
			<param key="campaignUri" />
		</return>
	</service>
	<service>
		<name>Campaign editor</name>
		<description>Edit campaign properties</description>
		<location url="/taoDelivery/SaSCampaign/sasEditInstance">
			<param key="uri" value="^campaignUri" />
			<param key="classUri" value="^campaignClassUri" />
		</location>
	</service>
	<service>
		<name>Campaign full editor</name>
		<description>Full edit form: edit campaign properties and select related deliveries</description>
		<location url="/taoDelivery/SaSCampaign/editCampaign">
			<param key="uri" value="^campaignUri" />
			<param key="classUri" value="^campaignClassUri" />
		</location>
	</service>
	<service>
		<name>Campaign class editor</name>
		<description>Edit a campaign (sub)class: add/edit/delete class properties</description>
		<location url="/taoDelivery/SaSCampaign/editCampaignClass">
			<param key="classUri" value="^campaignClassUri" />
		</location>
	</service>
	<service>
		<name>Campaign related delivery</name>
		<description>Select campaign deliveries</description>
		<location url="/taoDelivery/SaSCampaign/selectDeliveries">
			<param key="uri" value="^campaignUri" />
			<param key="classUri" value="^campaignClassUri" />
		</location>
	</service>
	<service>
		<name>Campaign removing</name>
		<description>Delete a campaign instance</description>
		<location url="/taoDelivery/SaSCampaign/sasDeleteInstance">
			<param key="uri"  value="^campaignUri" />
			<param key="classUri" value="^campaignClassUri" />
		</location>
	</service>
			
	<service>
		<name>ResultServers searching</name>
		<description>Search ResultServers instances</description>
		<location url="/taoDelivery/SaSResultServer/search" />
		<return>
			<param key="resultServerUris" />
		</return>
	</service>
	<service>
		<name>ResultServer add</name>
		<description>Add a new ResultServer and redirect to the edit form</description>
		<location url="/taoDelivery/SaSResultServer/sasAddInstance">
			<param key="classUri" value="^resultServerClassUri" />
		</location>
		<return>
			<param key="resultServerUri" />
		</return>
	</service>
	<service>
		<name>ResultServer editor</name>
		<description>Edit result server properties</description>
		<location url="/taoDelivery/SaSResultServer/sasEditInstance">
			<param key="uri" value="^resultServerUri" />
			<param key="classUri" value="^resultServerClassUri" />
		</location>
	</service>
	<service>
		<name>ResultServer full editor</name>
		<description>Full edit form: edit campaign properties and select related deliveries</description>
		<location url="/taoDelivery/SaSResultServer/editResultServer">
			<param key="uri" value="^resultServerUri" />
			<param key="classUri" value="^resultServerClassUri" />
		</location>
	</service>
	<service>
		<name>ResultServer class editor</name>
		<description>Edit a ResultServer (sub)class: add/edit/delete class properties</description>
		<location url="/taoDelivery/SaSResultServer/editResultServerClass">
			<param key="classUri" value="^resultServerClassUri" />
		</location>
	</service>
	<service>
		<name>ResultServer related delivery</name>
		<description>Select result server deliveries</description>
		<location url="/taoDelivery/SaSResultServer/selectDeliveries">
			<param key="uri" value="^resultServerUri" />
			<param key="classUri" value="^resultServerClassUri" />
		</location>
		<return>
			<param key="deliveryUris" />
		</return>
	</service>
	<service>
		<name>ResultServer removing</name>
		<description>Delete a ResultServer instance	</description>
		<location url="/taoDelievry/SaSResultServer/sasDeleteInstance">
			<param key="uri" value="^resultServerUri" />
			<param key="classUri" value="^resultServerClassUri" />
		</location>
	</service>
</services>