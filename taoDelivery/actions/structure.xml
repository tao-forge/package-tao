<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE extension SYSTEM "../../tao/doc/structure.dtd">
<extension name="Deliveries" url="/taoDelivery/Delivery/index" level="4">
	<description>Prepare, publish deliveries sessions and campaigns.</description>
	<sections>
		<section id="manage_deliveries" name="Manage deliveries" url="/taoDelivery/Delivery/index">
			<trees>
				<tree name="Deliveries library" 
					dataUrl="/taoDelivery/Delivery/getDeliveries" 
					editClassUrl="/taoDelivery/Delivery/editDeliveryClass"
					editInstanceUrl="/taoDelivery/Delivery/editDelivery"
					addInstanceUrl="/taoDelivery/Delivery/addDelivery"
					addSubClassUrl="/taoDelivery/Delivery/addDeliveryClass"
					deleteUrl="/taoDelivery/Delivery/delete"
					duplicateUrl="/taoDelivery/Delivery/cloneDelivery"
					moveInstanceUrl="/taoDelivery/Delivery/moveInstance"
				/>
			</trees>
			<actions>
				<action name="search" url="/taoDelivery/Delivery/search" context="*" />
				<action name="new class" js="GenerisAction.subClass" url="/taoDelivery/Delivery/addDeliveryClass" context="class" />
				<action name="new delivery" js="GenerisAction.instanciate" url="/taoDelivery/Delivery/addDelivery" context="class" />
				<action name="delete" js="GenerisAction.removeNode" url="/taoDelivery/Delivery/delete" context="resource" />
				<action name="duplicate" js="GenerisAction.duplicateNode" url="/taoDelivery/Delivery/cloneDelivery" context="instance" />
				<action name="move" js="GenerisAction.moveNode" url="" context="instance" />
				<action name="create table" url="/taoResults/Results/createTable"  context="class" />
				<action name="export" url="/tao/Export/index"  context="*" /> 
				<action name="import" url="/tao/Import/index"  context="class" /> 
				<action name="compile" url="/taoDelivery/Delivery/compileView" context="instance" />
				<action name="advanced mode" url="/taoDelivery/Delivery/advancedMode" context="instance" />
				<action name="simple mode" url="/taoDelivery/Delivery/simpleMode" context="instance" />
			</actions>
		</section>
		
		<section id="delivery_authoring" name="Advanced Authoring Tool" url="/taoDelivery/Delivery/authoring" />
		
		<section id="manage_campaigns" name="Manage campaigns" url="/taoDelivery/Campaign/index">
			<trees>
				<tree name="Campaigns library" 
					dataUrl="/taoDelivery/Campaign/getCampaigns" 
					editClassUrl="/taoDelivery/Campaign/editCampaignClass"
					editInstanceUrl="/taoDelivery/Campaign/editCampaign"
					addInstanceUrl="/taoDelivery/Campaign/addCampaign"
					addSubClassUrl="/taoDelivery/Campaign/addCampaignClass"
					deleteUrl="/taoDelivery/Campaign/delete"
					duplicateUrl="/taoDelivery/Campaign/cloneInstance"
					moveInstanceUrl="/taoDelivery/Campaign/moveInstance"
				/>
			</trees>
			<actions>
				<action name="search" url="/taoDelivery/Campaign/search" context="*" />
				<action name="new class" js="GenerisAction.subClass" url="/taoDelivery/Campaign/addCampaignClass" context="class" />
				<action name="new campaign" js="GenerisAction.instanciate" url="/taoDelivery/Campaign/addCampaign" context="class" />
				<action name="delete" js="GenerisAction.removeNode" url="/taoDelivery/Campaign/delete" context="resource" />
				<action name="duplicate" js="GenerisAction.duplicateNode" url="/taoDelivery/Campaign/cloneInstance" context="instance" />
				<action name="move" js="GenerisAction.moveNode" url="" context="instance" />
				<action name="create table" url="/taoResults/Results/createTable"  context="class" />
				<action name="export" url="/tao/Export/index"  context="*" /> 
				<action name="import" url="/tao/Import/index"  context="class" /> 
			</actions>
		</section>
		
		<section id="manage_resultservers" name="Manage result servers" url="/taoDelivery/ResultServer/index">
			<trees>
				<tree name="Result Servers library" 
					dataUrl="/taoDelivery/ResultServer/getResultServers" 
					editClassUrl="/taoDelivery/ResultServer/editResultServerClass"
					editInstanceUrl="/taoDelivery/ResultServer/editResultServer"
					addInstanceUrl="/taoDelivery/ResultServer/addResultServer"
					addSubClassUrl="/taoDelivery/ResultServer/addResultServerClass"
					deleteUrl="/taoDelivery/ResultServer/delete"
					duplicateUrl="/taoDelivery/ResultServer/cloneInstance"
					moveInstanceUrl="/taoDelivery/ResultServer/moveInstance"
				/>
			</trees>
			<actions>
				<action name="search" url="/taoDelivery/ResultServer/search" context="*" />
				<action name="new class" js="GenerisAction.subClass" url="/taoDelivery/ResultServer/addResultServerClass" context="class" />
				<action name="new result server" js="GenerisAction.instanciate" url="/taoDelivery/ResultServer/addResultServer" context="class" />
				<action name="delete" js="GenerisAction.removeNode" url="/taoDelivery/ResultServer/delete" context="resource" />
				<action name="duplicate" js="GenerisAction.duplicateNode" url="/taoDelivery/ResultServer/cloneInstance" context="instance" />
				<action name="move" js="GenerisAction.moveNode" url="" context="instance" />
				<action name="create table" url="/taoResults/Results/createTable"  context="class" />
				<action name="export" url="/tao/Export/index"  context="*" /> 
				<action name="import" url="/tao/Import/index"  context="class" /> 
			</actions>
		</section>
		
	</sections>
</extension>