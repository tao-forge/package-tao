<?xml version="1.0" encoding="utf-8"?>
<extension name="Delivery" url="/taoDelivery/Delivery/index" level="7">
	<sections>
		<section id="manage_deliveries" name="Manage deliveries" url="/taoDelivery/Delivery/index">
			<trees>
				<tree name="Deliveries library" 
					dataUrl="/taoDelivery/Delivery/getDeliveries" 
					editClassUrl="/taoDelivery/Delivery/editDeliveryClass"
					editInstanceUrl="/taoDelivery/Delivery/editDelivery"
					addInstanceUrl="/taoDelivery/Delivery/addDelivery"
					addSubClassUrl="/taoDelivery/Delivery/addDeliveryClass"
					deleteUrl="/taoDelivery/Delivery/delete"
					duplicateUrl="/taoDelivery/Delivery/cloneDelivery"
				/>
			</trees>
			<actions>
				<action name="new class" js="subClass" url="/taoDelivery/Delivery/addDeliveryClass" context="class" />
				<action name="new delivery" js="instanciate" url="/taoDelivery/Delivery/addDelivery" context="class" />
				<action name="delete" js="removeNode" url="/taoDelivery/Delivery/delete" context="resource" />
				<action name="duplicate" js="cloneNode" url="/taoDelivery/Delivery/cloneDelivery" context="instance" />
				<action name="edit delivery" url="/taoDelivery/Delivery/editDelivery" context="instance" />
				<action name="new property" js="addProperty" url="/taoDelivery/Delivery/addClassProperty" context="class" />
				<action name="create table" js="resultTable" url="/taoResults/Results/createTable"  context="class" />
				<action name="import" url="/taoDelivery/Delivery/import"  context="class" />
				<action name="export" url="/taoDelivery/Delivery/export"  context="class" reload="true" />
				<action name="view history" url="/taoDelivery/Delivery/viewHistory" context="instance" />
				<action name="add campaign" url="/taoDelivery/Delivery/addCompaign" context="resource" />
				<action name="add result server" url="/taoDelivery/Delivery/addResultServer" context="resource" />
			</actions>
		</section>
		
		<section id="manage_campaigns" name="Manage Campaigns" url="/taoDelivery/Campaign/index">
			<trees>
				<tree name="Campaigns library" 
					dataUrl="/taoDelivery/Campaign/getCampaigns" 
					editClassUrl="/taoDelivery/Campaign/editCampaignClass"
					editInstanceUrl="/taoDelivery/Campaign/editCampaign"
					addInstanceUrl="/taoDelivery/Campaign/addCampaign"
					addSubClassUrl="/taoDelivery/Campaign/addCampaignClass"
					deleteUrl="/taoDelivery/Campaign/delete"
					duplicateUrl="/taoDelivery/Campaign/cloneCampaign"
				/>
			</trees>
			<actions>
				<action name="new class" js="subClass" url="/taoDelivery/Campaign/addCampaignClass" context="class" />
				<action name="new campaign" js="instanciate" url="/taoDelivery/Campaign/addCampaign" context="class" />
				<action name="delete" js="removeNode" url="/taoDelivery/Campaign/delete" context="resource" />
				<action name="duplicate" js="cloneNode" url="/taoDelivery/Campaign/cloneCampaign" context="instance" />
				<action name="edit campaign" url="/taoDelivery/Campaign/editCampaign" context="instance" />
				<action name="new property" js="addProperty" url="/taoDelivery/Campaign/addClassProperty" context="class" />
				<action name="create table" js="resultTable" url="/taoResults/Results/createTable"  context="class" />
				<action name="import" url="/taoDelivery/Campaign/import"  context="class" />
				<action name="export" url="/taoDelivery/Campaign/export"  context="class" reload="true" />
			</actions>
		</section>
		
	</sections>
</extension>